{
  "version": 2,
  "name": "nest",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts"
    },
    {
      "src": "/api/swagger-ui.css",
      "dest": "/swagger-ui.css"
    },
    {
      "src": "/api/swagger-ui-bundle.js",
      "dest": "/swagger-ui-bundle.js"
    },
    {
      "src": "/api/swagger-ui-standalone-preset.js",
      "dest": "/swagger-ui-standalone-preset.js"
    }
  ]
}
